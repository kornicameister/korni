{
  "name": "src.kornicameister.github.io",
  "homepage": "http://kornicameister.github.io/korni",
  "version": "0.2.6",
  "private": true,
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "yarn prettier",
      "yarn lint",
      "git add"
    ]
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "precommit": "lint-staged",
    "start": "react-scripts-ts start",
    "analyze": "source-map-explorer build/static/js/main.*",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom --verbose",
    "coverage": "react-scripts-ts test --env=jsdom --silent --coverage",
    "lint": "tslint -c tslint.json './src/**/*.{ts,tsx}'",
    "prettier": "prettier --write --config .prettierrc './src/**/*.{ts,tsx}'",
    "eject": "react-scripts-ts eject"
  },
  "dependencies": {
    "bootstrap": "4.0.0-beta",
    "classnames": "^2.2.5",
    "font-awesome": "^4.7.0",
    "jsonp": "^0.2.1",
    "moment": "^2.20.1",
    "react": "^16.2.0",
    "react-autosuggest": "^9.3.2",
    "react-dom": "^16.2.0",
    "react-ga": "2.3.5",
    "react-google-charts": "^1.5.5",
    "react-loadable": "^5.3.1",
    "react-router-dom": "^4.2.2",
    "react-scripts-ts": "^2.8.0",
    "react-transition-group": "^2.2.1",
    "reactstrap": "^5.0.0-alpha.4",
    "typeface-roboto": "^0.0.45",
    "typescript": "^2.6.2",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.3",
    "@types/enzyme": "^3.1.6",
    "@types/google.visualization": "^0.0.41",
    "@types/jest": "^21.1.9",
    "@types/jsonp": "^0.2.0",
    "@types/react-autosuggest": "^9.3.2",
    "@types/react-dom": "^16.0.3",
    "@types/react-loadable": "^5.3.0",
    "@types/react-router-dom": "^4.2.3",
    "@types/reactstrap": "^5.0.6",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "gh-pages": "^1.1.0",
    "husky": "^0.14.3",
    "jest-enzyme": "^4.0.1",
    "lint-staged": "^6.0.0",
    "prettier": "^1.9.2",
    "react-test-renderer": "^16.2.0",
    "source-map-explorer": "^1.5.0",
    "ts-jest": "^22.0.0",
    "tslint": "^5.8.0",
    "tslint-react": "^3.2.0"
  },
  "peerDependencies": {
    "jest": "^22.0.4",
    "jquery": "^3.2.1",
    "popper.js": "^1.12.9",
    "react-popper": "^0.7.4"
  }
}
