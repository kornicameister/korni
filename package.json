{
  "name": "src.kornicameister.github.io",
  "homepage": "http://kornicameister.github.io/korni",
  "version": "0.2.12",
  "private": true,
  "engines": {
    "node": "9.11.1"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "yarn prettier",
      "yarn lint",
      "git add"
    ]
  },
  "jest": {
    "coverageReporters": [
      "lcov",
      "text"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.spec.{ts,tsx}",
      "!src/types/*.ts"
    ]
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "precommit": "lint-staged",
    "start": "react-scripts-ts start",
    "analyze": "source-map-explorer build/static/js/main.*",
    "build": "./scripts/build.sh",
    "test": "react-scripts-ts test --env=jsdom --verbose",
    "coverage": "./scripts/coverage.sh",
    "lint": "tslint -c tslint.json './src/**/*.{ts,tsx}'",
    "prettier": "prettier --write --config .prettierrc './src/**/*.{ts,tsx}'",
    "eject": "react-scripts-ts eject"
  },
  "dependencies": {
    "bootstrap": "4.0.0-beta",
    "classnames": "^2.2.5",
    "firebase": "4.12.0",
    "font-awesome": "^4.7.0",
    "jsonp": "^0.2.1",
    "moment": "^2.21.0",
    "react": "^16.3.1",
    "react-autosuggest": "^9.3.4",
    "react-dom": "^16.3.1",
    "react-ga": "2.5.3",
    "react-google-charts": "2.0.20",
    "react-loadable": "^5.3.1",
    "react-router-dom": "^4.2.2",
    "react-scripts-ts": "^2.14.0",
    "react-transition-group": "^2.3.0",
    "typeface-roboto": "^0.0.54",
    "typescript": "^2.8.1",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.3",
    "@types/enzyme": "^3.1.9",
    "@types/google.visualization": "0.0.43",
    "@types/jest": "23.3.2",
    "@types/jsonp": "^0.2.0",
    "@types/react-autosuggest": "^9.3.3",
    "@types/react-dom": "^16.0.3",
    "@types/react-loadable": "^5.3.0",
    "@types/react-router-dom": "^4.2.6",
    "@types/reactstrap": "6.0.3",
    "codacy-coverage": "3.0.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "husky": "1.0.0",
    "jest-enzyme": "6.0.2",
    "lint-staged": "7.2.2",
    "prettier": "^1.11.1",
    "react-test-renderer": "^16.3.0",
    "source-map-explorer": "^1.5.0",
    "ts-jest": "^22.4.2",
    "tslint": "^5.9.1",
    "tslint-react": "^3.5.1"
  }
}
