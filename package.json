{
  "name": "src.kornicameister.github.io",
  "homepage": "http://kornicameister.github.io/korni",
  "version": "0.2.0",
  "private": true,
  "lint-staged": {
    "src/**/*.{js,jsx,json,css,ts,tsx}": [
      "yarn lint",
      "git add"
    ]
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "precommit": "lint-staged",
    "start": "react-scripts-ts start",
    "analyze": "source-map-explorer build/static/js/main.*",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom --verbose",
    "coverage": "react-scripts-ts test --env=jsdom --silent --coverage",
    "lint": "tslint -c tslint.json './src/**/*.{ts,tsx}'",
    "eject": "react-scripts-ts eject"
  },
  "dependencies": {
    "bootstrap": "4.0.0-beta",
    "classnames": "^2.2.5",
    "jsonp": "^0.2.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-loadable": "^5.3.1",
    "react-router-dom": "^4.2.2",
    "react-scripts-ts": "^2.8.0",
    "react-transition-group": "^2.2.1",
    "reactstrap": "^5.0.0-alpha.3",
    "typeface-roboto": "^0.0.41"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.3",
    "@types/enzyme": "^2.8.11",
    "@types/jest": "^21.1.4",
    "@types/react-dom": "^16.0.2",
    "@types/jsonp": "^0.2.0",
    "@types/react-loadable": "^4.0.4",
    "@types/react-router-dom": "^4.0.8",
    "@types/reactstrap": "^4.6.4",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "gh-pages": "^1.0.0",
    "husky": "^0.14.3",
    "jest-enzyme": "^4.0.0",
    "lint-staged": "^4.3.0",
    "react-test-renderer": "^16.0.0",
    "source-map-explorer": "^1.5.0",
    "tslint": "^5.7.0",
    "tslint-react": "^3.2.0"
  }
}
